{% extends 'base.html.twig' %}

{% block title %}Edit Entry{% endblock %}

{% block body %}
    <h1>Edit Entry</h1>

    {{ form_start(entryForm) }}
    <div class="input-fields">
        {% if entryForm.workday is defined %}
            {{ form_row(entryForm.workday) }}
        {% endif %}
        {{ form_row(entryForm.note) }}
    </div>
    {{ form_end(entryForm) }}
{% endblock %}
